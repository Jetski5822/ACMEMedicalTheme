@{
    var shapeType = string.Format("SideNavigation_Menu");
    Model.Menu.Metadata.Alternates.Clear();
    Model.Menu.Metadata.Type = shapeType;
    Model.Menu.Metadata.BindingSources.Clear();
    Model.Menu.Metadata.DisplayType = "";
}
<div class="navbar-header">
    <button class="navbar-toggle pull-right" type="button" data-toggle="collapse" data-target="#sidenavigation-menu">
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
    </button>
</div>
<div class="navbar-collapse collapse" id="sidenavigation-menu">
    @Display(Model.Menu)
</div>